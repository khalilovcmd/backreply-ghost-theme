{{!< default}}
<div class="gh-page">
    <div class="gh-container gh-page-with-sidebar">

        <div class="gh-page-main">
            <div class="gh-postfeed">
            {{#foreach posts}}

                {{> "card" page=../pagination.page }} {{!-- partials/card.hbs --}}

            {{/foreach}}
            </div>

            {{!-- If JS isn't enabled, fallback to standard pagination --}}
            <noscript>
                <div class="gh-pagination">
                    {{pagination}}
                </div>
            </noscript>
        </div>

        {{> "sidebar"}}

    </div>
</div>
